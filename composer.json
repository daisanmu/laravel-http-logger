{
    "name": "daisanmu/laravel-http-logger",
    "description": "A Laravel 5 package to log HTTP requests",
    "keywords": [
        "daisanmu",
        "laravel-http-logger"
    ],
    "homepage": "https://github.com/daisanmu/laravel-http-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "royeedai@gmail.com",
            "homepage": "https://github.com/daisanmu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.6.0|~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0|~3.7.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Daisanmu\\HttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daisanmu\\HttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    }
}
